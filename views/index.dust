    <link rel="stylesheet" href="components/paper-styles/demo.css">
    <style is="custom-style">
        paper-scroll-header-panel {
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            background-color: var(--paper-grey-200, #eee);;
        }
        .menu {
            flex-grow: 2;
        }
        .title {
            flex-grow: 1;
        }
        .ending {
            flex-grow: 1;
        }
    </style>
</head>
<body>
<paper-scroll-header-panel fixed>
    <paper-toolbar>
        <div class="title"> {message} </div>
        <paper-tabs class="menu">
            <paper-tab>
                Home
            </paper-tab>
            <paper-tab>
                Social
            </paper-tab>
            <paper-tab>
                Sponsors
            </paper-tab>
            <paper-tab>
                Speakers
            </paper-tab>
            <paper-tab>
                Talks
            </paper-tab>
        </paper-tabs>
        <div class="ending"></div>
    </paper-toolbar>
</paper-scroll-header-panel>


</body>
